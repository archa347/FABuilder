automata a1 (
	(q0, q1, q2, q3, q4, q5, q6, q7, q8, q9),
	(a,b),
	q0,
	(q6,q9)) {
	q0 : { 
		(&, q1)
		(&, q2)
		(&, q3)
		}
	q1 : { 
		(a, q4)
		(a, q5)
		(b, q6)
		}
	q2 : {
		(b, q6)
		(a, q9)
		}
	q3 : {
		(b, q7)
		(b, q8)
		(a, q9)
		}
	q4 : {
		(b, q6)
		(a, q5)
		}
	q5 : {
		(b,q6)
		(a,q4)
		}
		
	q6 : {
		(a,q4)
		(b,q6)
		}
	
	q7 : { 
		(a, q8)
		(b, q9)
		}
	
	q8 : {
		(a, q9)
		(b, q7)
		}
	q9 : {
		(a, q9)
		(b,q7)
		}
	}